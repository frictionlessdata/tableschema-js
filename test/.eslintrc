---
environment:
  es5
parserOptions:
  sourceType: script
rules:
  comma-style:
    - 2
    - first
  one-var:
    - 1
    - var: always
      let: always
      const: always
  no-use-before-define:
    - 2
    - nofunc
  no-var:
    - 0
  prefer-arrow-callback:
    - 0
  func-names:
    - 0
  vars-on-top:
    - 0